PostInstScript: <<
set -e

if [ "$1" = "configure" ]; then
    if ! dpkg-statoverride --list %p/var/lib/php/sessions >/dev/null; then
        dpkg-statoverride --update --quiet --add root root 01733 %p/var/lib/php/sessions
    fi
fi

# Remove old registry directory
if [ -d "%p/var/lib/php/modules/registry" ]; then
    rm -r %p/var/lib/php/modules/registry;
fi

exit 0
<<
PostRmScript: <<
# postrm script for %N
#
# see: dh_installdeb(1)

set -e

if [ "$1" = "purge" ]; then
    rm -rf %p/var/lib/php
fi

exit 0
<<
PreRmScript: <<
set -e

if [ "$1" = "remove" -o "$1" = "purge" ]; then
    if dpkg-statoverride --list %p/var/lib/php/sessions >/dev/null; then
	dpkg-statoverride --remove %p/var/lib/php/sessions;
    fi
fi

exit 0
<<
